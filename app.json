{
  "expo": {
    "name": "Movever",
    "slug": "movever",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./AppLogo.png",
    "scheme": "movever",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./AppLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.movever.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./AppLogo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "INTERNET"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.movever.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./AppLogo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "@react-native-community/datetimepicker",
      [
        "expo-notifications",
        {
          "icon": "./AppLogo.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "970fca7e-e44a-4eb1-af1e-49374007fc37"
      }
    }
  }
}